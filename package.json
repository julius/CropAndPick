{
  "name": "CropAndPick",
  "version": "1.0.0",
  "description": "Quickly Crop Images And Pick Colors",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . CropAndPick --platform=win32 --arch=x64 --asar --overwrite --out=./out",
    "package-win32": "electron-packager . CropAndPick --platform=win32 --arch=x64 --asar --overwrite --out=./out",
    "package-linux": "electron-packager . CropAndPick --platform=linux --arch=x64 --asar --overwrite --out=./out && cp linux_specific/* out/CropAndPick-linux-x64"
  },
  "repository": "https://github.com/julius/CropAndPick",
  "keywords": [
    "CropAndPick",
    "crop",
    "images",
    "pick",
    "colors"
  ],
  "author": "Julius Eckert",
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-packager": "^14.0.0"
},
  "dependencies": {}
}
